class CondLoopsExercise4
{
  public static void main (String[] args)
  {
    String pass;
    
    do
    {
      System.out.print("Enter Password: ");
      pass = In.getString();
    }
    while (!pass ==  "ladadida");
    
    System.out.println("That is the Password");
    
  }
}